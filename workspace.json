{
  "version": 1,
  "projects": {
    "harika-web": {
      "root": "apps/harika-web",
      "sourceRoot": "apps/harika-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/harika-web",
            "index": "apps/harika-web/src/index.html",
            "main": "apps/harika-web/src/main.tsx",
            "polyfills": "apps/harika-web/src/polyfills.ts",
            "tsConfig": "apps/harika-web/tsconfig.app.json",
            "assets": [
              "apps/harika-web/src/favicon.ico",
              "apps/harika-web/src/assets"
            ],
            "styles": ["apps/harika-web/src/styles.css"],
            "scripts": [],
            "webpackConfig": "apps/harika-web/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/harika-web/src/environments/environment.ts",
                  "with": "apps/harika-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "harika-web:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "harika-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/harika-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "harika-web-e2e": {
      "root": "apps/harika-web-e2e",
      "sourceRoot": "apps/harika-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/harika-web-e2e/cypress.json",
            "tsConfig": "apps/harika-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "harika-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "harika-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "harika-notes": {
      "root": "libs/harika-notes",
      "sourceRoot": "libs/harika-notes/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/harika-notes/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/harika-notes/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "harika-mobile": {
      "root": "apps/harika-mobile",
      "sourceRoot": "apps/harika-mobile/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "start": {
          "builder": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start harika-mobile"
          }
        },
        "run-ios": {
          "builder": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "builder": "@nrwl/react-native:bundle",
          "outputs": ["apps/harika-mobile/build"],
          "options": {
            "entryFile": "apps/harika-mobile/index.js",
            "platform": "ios",
            "bundleOutput": "dist/apps/harika-mobile/ios/index.bundle"
          }
        },
        "run-android": {
          "builder": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "builder": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/harika-mobile/android/app/build/outputs/bundle",
            "apps/harika-mobile/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "builder": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/harika-mobile/index.js",
            "platform": "android",
            "bundleOutput": "dist/apps/harika-mobile/android/index.bundle"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-mobile/**/*.{js,ts,tsx}"]
          }
        },
        "sync-deps": {
          "builder": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/harika-mobile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "harika-core": {
      "root": "libs/harika-core",
      "sourceRoot": "libs/harika-core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/harika-core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/harika-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "harika-web"
}
