{
  "version": 1,
  "projects": {
    "harika-web": {
      "root": "apps/harika-web",
      "sourceRoot": "apps/harika-web/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/harika-web",
            "index": "apps/harika-web/src/index.html",
            "main": "apps/harika-web/src/main.tsx",
            "polyfills": "apps/harika-web/src/polyfills.ts",
            "tsConfig": "apps/harika-web/tsconfig.app.json",
            "assets": [
              "apps/harika-web/src/favicon.ico",
              "apps/harika-web/src/manifest.json",
              "apps/harika-web/src/assets"
            ],
            "styles": ["apps/harika-web/src/styles.css"],
            "scripts": [],
            "webpackConfig": "apps/harika-web/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/harika-web/src/environments/environment.ts",
                  "with": "apps/harika-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "harika-web:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "harika-web:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/harika-web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "harika-web-e2e": {
      "root": "apps/harika-web-e2e",
      "sourceRoot": "apps/harika-web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/harika-web-e2e/cypress.json",
            "tsConfig": "apps/harika-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "harika-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "harika-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-web-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "harika-core": {
      "root": "libs/harika-core",
      "sourceRoot": "libs/harika-core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/harika-core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/harika-core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "generate": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config libs/harika-ui/codegen.yml"
              }
            ]
          }
        }
      }
    },
    "harika-mobile-ionic": {
      "root": "apps/harika-mobile-ionic",
      "sourceRoot": "apps/harika-mobile-ionic/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/harika-mobile-ionic",
            "index": "apps/harika-mobile-ionic/src/index.html",
            "main": "apps/harika-mobile-ionic/src/main.tsx",
            "polyfills": "apps/harika-mobile-ionic/src/polyfills.ts",
            "tsConfig": "apps/harika-mobile-ionic/tsconfig.app.json",
            "assets": [
              "apps/harika-mobile-ionic/src/assets",
              "apps/harika-mobile-ionic/src/manifest.json"
            ],
            "styles": ["apps/harika-mobile-ionic/src/styles.css"],
            "scripts": [],
            "webpackConfig": "apps/harika-mobile-ionic/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/harika-mobile-ionic/src/environments/environment.ts",
                  "with": "apps/harika-mobile-ionic/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "harika-mobile-ionic:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "harika-mobile-ionic:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/harika-mobile-ionic/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/harika-mobile-ionic/jest.config.js",
            "passWithNoTests": true
          }
        },
        "cap": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "--help"
          }
        },
        "add": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "add",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "add ios"
            },
            "android": {
              "cmd": "add android"
            }
          }
        },
        "copy": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "copy",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "copy ios"
            },
            "android": {
              "cmd": "copy android"
            }
          }
        },
        "open": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "open",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "open ios"
            },
            "android": {
              "cmd": "open android"
            }
          }
        },
        "sync": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "sync",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "sync ios"
            },
            "android": {
              "cmd": "sync android"
            }
          }
        },
        "update": {
          "builder": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "update",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "update ios"
            },
            "android": {
              "cmd": "update android"
            }
          }
        }
      }
    },
    "harika-mobile-ionic-e2e": {
      "root": "apps/harika-mobile-ionic-e2e",
      "sourceRoot": "apps/harika-mobile-ionic-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/harika-mobile-ionic-e2e/cypress.json",
            "tsConfig": "apps/harika-mobile-ionic-e2e/tsconfig.e2e.json",
            "devServerTarget": "harika-mobile-ionic:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "harika-mobile-ionic:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/harika-mobile-ionic-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "harika-ui": {
      "root": "libs/harika-ui",
      "sourceRoot": "libs/harika-ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/harika-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/harika-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "harika-web"
}
